[application]

config/name="Battle Viewer 3D"
config/description="Modern battle simulation viewer with rich graphics"
config/features=PackedStringArray("4.2", "Forward Plus")
run/main_scene="res://scenes/BattleViewer.tscn"

[autoload]

[display]

window/size/viewport_width=1400
window/size/viewport_height=900
window/size/resizable=true

[rendering]

renderer/rendering_method="forward_plus"
renderer/rendering_method.mobile="forward_plus"
textures/canvas_textures/default_texture_filter=2
anti_aliasing/quality/msaa_3d=1
anti_aliasing/quality/screen_space_aa=1
global_illumination/gi/use_half_resolution=true
environment/defaults/default_clear_color=Color(0.1, 0.12, 0.16, 1.0)
occlusion_culling/use_occlusion_culling=true

[physics]

common/physics_ticks_per_second=120
common/max_physics_steps_per_frame=8

[debug]

settings/stdout/print_fps=false
